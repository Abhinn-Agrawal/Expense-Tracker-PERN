generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Income {
  id          Int      @id @default(autoincrement())
  title       String
  amount      Int
  date        DateTime
  category    String
  description String?
}

model Expense {
  id          Int      @id @default(autoincrement())
  title       String
  amount      Int
  date        DateTime
  category    String
  description String?
}